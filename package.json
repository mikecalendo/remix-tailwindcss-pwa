{
  "private": true,
  "name": "remix-pwa",
  "description": "",
  "license": "",
  "scripts": {
    "typecheck": "tsc",
    "build": "run-p build:*",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev": "run-p dev:*",
    "dev:remix": "dotenv -- remix dev",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "postinstall": "remix setup node"
  },
  "devDependencies": {
    "@remix-run/react": "1.1.3",
    "@remix-run/serve": "1.1.3",
    "awsm.css": "3.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "1.1.3",
    "turbo": "^1.1.2"
  },
  "dependencies": {
    "@remix-run/dev": "1.1.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "autoprefixer": "10.4.2",
    "cross-env": "7.0.3",
    "dev-kong": "0.11.0",
    "dotenv-cli": "4.1.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-react": "7.28.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "7.3.1",
    "precss": "4.0.0",
    "prettier": "2.5.1",
    "react-icons": "4.3.1",
    "tailwindcss": "3.0.18",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": true
}
